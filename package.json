    "nestjs-redis": "^1.2.8",
    "opentype.js": "^1.3.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pluralize": "^8.0.0",
    "rate-limit-redis": "^2.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.10",
    "resize-img": "^2.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "shutterstock-api": "^1.0.24",
    "spreadsheet-to-json": "^2.0.0",
    "ssml-check-core": "^0.3.2",
    "ssml-split": "^0.5.0",
    "svg-path-bounds": "^1.0.1",
    "swagger-ui-express": "^4.1.4",
    "tmp": "^0.2.1",
