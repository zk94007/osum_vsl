{
  "name": "osum-back-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node --max-old-space-size=4096 node_modules/.bin/nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node -r ts-node/register -- src/main.ts",
    "start:inspect": "node --inspect=127.0.0.1:9223 -r ts-node/register -- src/main.ts",
    "start:dev": "nest start --watch --webpack",
    "start:debug": "nest start --debug --watch --webpack",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.1.2",
    "@google-cloud/text-to-speech": "^3.1.0",
    "@google-cloud/video-intelligence": "^3.0.1",
    "@nestjs/bull": "^0.1.2",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjsx/api-version": "^0.1.1",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "auth0": "^2.28.0",
    "bull": "^3.16.0",
    "bull-board": "^0.9.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "csv-stringify": "^5.5.0",
    "express-rate-limit": "^5.1.3",
    "ffprobe-client": "^1.1.6",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^3.0.0",
    "get-video-dimensions": "^1.0.0",
    "google-auth-library": "^6.0.6",
